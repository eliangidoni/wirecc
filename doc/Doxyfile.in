PROJECT_NAME = @PROJECT_NAME@-@wirecc_VERSION_MAJOR@.@wirecc_VERSION_MINOR@
INPUT = @PROJECT_SOURCE_DIR@/include \
        @PROJECT_SOURCE_DIR@/include/@PROJECT_NAME@_config.h.in
OUTPUT_DIRECTORY = doc
RECURSIVE = YES
EXTRACT_ALL = YES
EXTRACT_LOCAL_CLASSES = NO
HIDE_UNDOC_MEMBERS = YES
HIDE_UNDOC_CLASSES = YES
GENERATE_TREEVIEW = YES
DISABLE_INDEX = YES
SHOW_FILES = NO
SHOW_NAMESPACES = YES
VERBATIM_HEADERS = NO
EXCLUDE_SYMBOLS =

EXTRACT_MACROS         = YES
ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = YES
PREDEFINED             = WIRECC_DISABLE_COPY_AND_ASSIGN(type)